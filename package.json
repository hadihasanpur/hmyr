        "tailwindcss": "^3.4.7",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "bootstrap": "^4.5.0",
        "bootstrap-select": "^1.13.18",
        "chart.js": "^2.9.4",
        "jquery": "^3.5.1",
